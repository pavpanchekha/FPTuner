(FPCore (a b2 c)
        :name "nmse_problem_3_2_1_negative"
        :cite (hamming-1987 herbie-2015)
        :fpbench-domain textbook
        :pre (and (>= (* b2 b2) (* a c))
                  (<= 1 a 5)
                  (<= 11 b2 12)
                  (<= 0 c 5))

        (/ (- (- b2) (sqrt (- (* b2 b2) (* a c)))) a))
