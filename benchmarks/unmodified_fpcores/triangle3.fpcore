;; -*- mode: scheme -*-

(FPCore (a b c)
        :name "triangle3"
        :cite (darulova-kuncak-2014)
        :pre (and (<= 1 a 9) (<= 1 b 9) (<= 1 c 9)
                  (> (+ a b) (+ c 1e-3)) (> (+ a c) (+ b 1e-3)) (> (+ b c) (+ a 1e-3)))
        (let ([s (/ (+ (+ a b) c) 2)])
          (sqrt (* (* (* s (- s a)) (- s b)) (- s c)))))
